{
  "name": "applybotx",
  "version": "1.0.0",
  "description": "AI-powered job application email generator",
  "scripts": {
    "dev": "cd frontend && npm run dev",
    "dev:backend": "cd backend && python -m uvicorn app.main:app --reload --port 8000",
    "build": "cd frontend && npm run build",
    "start": "concurrently \"npm run dev:backend\" \"npm run dev\"",
    "install:frontend": "cd frontend && npm install",
    "install:backend": "cd backend && pip install -r requirements.txt",
    "setup": "npm run install:frontend && npm run install:backend"
  },
  "keywords": ["ai", "job-application", "email-generator"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
